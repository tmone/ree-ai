# REE AI Environment Variables
# Copy this file to .env and fill in your values

# ==============================================================================
# REQUIRED
# ==============================================================================

# OpenAI API Key (GET FROM: https://platform.openai.com/api-keys)
OPENAI_API_KEY=sk-your-openai-api-key-here

# ==============================================================================
# OPTIONAL (defaults provided)
# ==============================================================================

# PostgreSQL Database (Q1, Q4: Context Memory)
POSTGRES_DB=ree_ai
POSTGRES_USER=ree_ai_user
POSTGRES_PASSWORD=ree_ai_pass_2025

# Open WebUI Secret Key (change in production!)
WEBUI_SECRET_KEY=your-secret-key-change-in-production

# OpenSearch Password (required by OpenSearch 2.11.0+)
OPENSEARCH_PASSWORD=Admin123!@#

# Debug Settings
DEBUG=true
LOG_LEVEL=INFO

# LangSmith Monitoring (Optional)
LANGCHAIN_TRACING_V2=false
LANGCHAIN_API_KEY=

# ==============================================================================
# Google Cloud Storage (for property images)
# ==============================================================================

# GCS Bucket name for storing property images
GCS_BUCKET_NAME=ree-ai-property-images

# GCS Project ID
GCS_PROJECT_ID=your-gcp-project-id

# Path to service account credentials JSON file
# Download from: Google Cloud Console > IAM > Service Accounts > Keys
GOOGLE_APPLICATION_CREDENTIALS=/path/to/your/service-account.json
